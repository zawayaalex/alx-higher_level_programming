#!/usr/bin/python3
def max_integer(my_list=[]):
    m = 0
    if len(my_list) == 0:
        return None
    else:
        for i in range(0, len(my_list)-1, 1):
            if my_list[i] < my_list[i + 1]:
                m = my_list[i + 1]
            else:
                m = my_list[i]
        return m
